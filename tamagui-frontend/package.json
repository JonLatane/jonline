{
  "name": "myapp-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach -pi run watch",
    "web": "yarn build && cd apps/next && yarn next",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "postinstall": "yarn build",
    "build": "yarn workspaces foreach --exclude next-app -pi run build",
    "upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader && manypkg upgrade react-native-web-lite",
    "protos": "protoc --plugin=\"./node_modules/.bin/protoc-gen-ts_proto\" -I ../protos/ --ts_proto_opt=esModuleInterop=true --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=outputClientImpl=grpc-web --ts_proto_opt=lowerCaseServiceMethods=true --ts_proto_out=\"./packages/app/generated\" ../protos/jonline.proto"
  },
  "resolutions": {
    "esbuild": "0.15.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "0.18.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.19.1",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "ts-proto": "^1.138.0",
    "ts-protoc-gen": "^0.15.0",
    "turbo": "^1.4.2",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.3"
}
