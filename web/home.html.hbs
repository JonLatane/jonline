<html>
  <head>
    <title>Jonline</title>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, user-scalable=no"
    />
    <link rel="stylesheet" href="styles.css" />
  </head>
  <body class="is-preload">

    <!-- Wrapper -->
    <div id="wrapper">

      <!-- Main -->
      <div id="main">
        <div class="inner">

          <!-- Header -->
          <header id="header">
            <a href="index.html" class="logo">Built with
              <strong>Editorial</strong>
              by HTML5 UP</a>
            <ul class="icons">
            </ul>
          </header>

          <!-- Content -->
          <section>
            <header class="main">
              <h1>Jonline: Web Interface Demo</h1>
              <p>The following content was generated from the Globally Visible
                Posts and Groups at
                <a href="/app">this Jonline instance</a>.</p>
              <p>Eventually, Jonline will support many templates and
                customizations.</p>
            </header>

            {{#each top_posts}}
              {{#with this}}
                <section>
                  <h2 class="post_title">{{title}}</h2>
                  <h6 class="post_link"><a href="{{link}}">{{link}}</a></h6>
                  {{{content}}}
                </section>
              {{/with}}
            {{/each}}
          </section>

        </div>
      </div>

      <!-- Sidebar -->
      <div id="sidebar">
        <div class="inner">

          <!-- Menu -->
          <nav id="menu">
            <header class="major">
              <h2>Menu</h2>
            </header>
            <ul>
              <li><a href="{{home}}">Home</a></li>
              <li>
                <span class="opener active">Groups</span>
                <ul>
                  {{#each groups}}
                    <li><a href="#">{{this.name}}</a></li>
                  {{/each}}
                </ul>
              </li>
              <li><a href="/app">App</a></li>
            </ul>
          </nav>
        </div>
      </div>

    </div>

    <!-- Scripts -->
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/browser.min.js"></script>
    <script src="assets/js/breakpoints.min.js"></script>
    <script src="assets/js/util.js"></script>
    <script src="assets/js/main.js"></script>

  </body>
</html>